{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Nest App",
        "type": "node",
        "request": "launch",
        "runtimeArgs": ["--nolazy", "--inspect-brk"],
        "program": "${workspaceFolder}/node_modules/@nestjs/cli/bin/nest.js",
        "args": ["start", "--watch"],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  }
  